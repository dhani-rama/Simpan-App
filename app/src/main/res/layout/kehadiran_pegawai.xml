<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/kehadiran_pegawai"
    tools:context=".user.info_umum.KehadiranPegawai">

    <Button
        android:id="@+id/btn_back22"
        android:layout_width="53dp"
        android:layout_height="53dp"
        android:background="@drawable/btn_back_baru"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.013"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.005" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="207dp"
        android:layout_height="28dp"
        android:background="@drawable/tanggal"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.176"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.103" />

    <TextView
        android:id="@+id/jml_pns"
        android:layout_width="195dp"
        android:layout_height="33dp"
        android:layout_marginStart="4dp"
        android:background="@drawable/jml_pns"
        android:text="20"
        android:paddingTop="5dp"
        android:paddingLeft="155dp"
        android:textSize="16sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.166" />

    <TextView
        android:id="@+id/jml_non_pns"
        android:layout_width="207dp"
        android:layout_height="32dp"
        android:background="@drawable/jml_non_pns"
        android:paddingLeft="170dp"
        android:paddingTop="5dp"
        android:text="20"
        android:textSize="16sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.75"
        app:layout_constraintStart_toEndOf="@+id/jml_pns"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.166" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="221dp"
        android:layout_height="34dp"
        android:layout_marginTop="36dp"
        android:background="@drawable/detail_kehadiran"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/jml_pns" />

    <TextView
        android:id="@+id/HadirFinger"
        android:layout_width="299dp"
        android:layout_height="67dp"
        android:layout_marginTop="16dp"
        android:background="@drawable/hadir_finger"
        android:text="20"
        android:textColor="#000000"
        android:textSize="18dp"
        android:paddingTop="22dp"
        android:paddingLeft="250dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView7" />

    <TextView
        android:id="@+id/sakit"
        android:layout_width="299dp"
        android:layout_height="67dp"
        android:layout_marginTop="12dp"
        android:background="@drawable/sakit"
        android:paddingLeft="250dp"
        android:paddingTop="22dp"
        android:text="20"
        android:textColor="#000000"
        android:textSize="18dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/cutti" />

    <TextView
        android:id="@+id/HadirManual"
        android:layout_width="299dp"
        android:layout_height="67dp"
        android:layout_marginTop="12dp"
        android:background="@drawable/hadir_manual"
        android:paddingLeft="250dp"
        android:paddingTop="22dp"
        android:text="20"
        android:textColor="#000000"
        android:textSize="18dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/HadirFinger" />

    <TextView
        android:id="@+id/DinasLuar"
        android:layout_width="299dp"
        android:layout_height="67dp"
        android:layout_marginTop="12dp"
        android:background="@drawable/dinas_luar"
        android:paddingLeft="250dp"
        android:paddingTop="22dp"
        android:text="20"
        android:textColor="#000000"
        android:textSize="18dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/HadirManual" />

    <TextView
        android:id="@+id/cutti"
        android:layout_width="299dp"
        android:layout_height="67dp"
        android:layout_marginTop="12dp"
        android:background="@drawable/cutti"
        android:paddingLeft="250dp"
        android:paddingTop="22dp"
        android:text="20"
        android:textColor="#000000"
        android:textSize="18dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/DinasLuar" />

    <TextView
        android:id="@+id/izin"
        android:layout_width="299dp"
        android:layout_height="67dp"
        android:layout_marginTop="12dp"
        android:background="@drawable/izin"
        android:paddingLeft="250dp"
        android:paddingTop="22dp"
        android:text="20"
        android:textColor="#000000"
        android:textSize="18dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/sakit"
        app:layout_constraintVertical_bias="0.09" />
</androidx.constraintlayout.widget.ConstraintLayout>